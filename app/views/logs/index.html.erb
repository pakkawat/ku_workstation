<div class="panel panel-danger">
  <!-- Default panel contents -->
  <div class="panel-heading">Error Logs</div>
  <br>
    <table id="" class="datatable_display table table-striped">
      <thead>
        <tr>
          <th>Ku ID</th>
          <th>Firstname</th>
          <th>Lastname</th>
          <th>View Log</th>
        </tr>
      </thead>
      <tbody>
        <%= render partial: "errorlog", collection: @errors, as: :log %>
      </tbody>
  </table>
</div>


<div class="panel panel-info">
  <!-- Default panel contents -->
  <div class="panel-heading">Logs</div>
  <br>
    <table id="" class="datatable_display table table-striped">
      <thead>
        <tr>
          <th>Ku ID</th>
          <th>Firstname</th>
          <th>Lastname</th>
          <th>View Log</th>
        </tr>
      </thead>
      <tbody>
        <%= render partial: "passlog", collection: @passes, as: :log %>
      </tbody>
  </table>
</div>