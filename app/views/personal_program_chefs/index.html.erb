<p id="notice"><%= notice %></p>

<h1>Listing Personal Program Chefs</h1>

<table>
  <thead>
    <tr>
      <th>Personal chef resource</th>
      <th>Personal program</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personal_program_chefs.each do |personal_program_chef| %>
      <tr>
        <td><%= personal_program_chef.personal_chef_resource_id %></td>
        <td><%= personal_program_chef.personal_program_id %></td>
        <td><%= link_to 'Show', personal_program_chef %></td>
        <td><%= link_to 'Edit', edit_personal_program_chef_path(personal_program_chef) %></td>
        <td><%= link_to 'Destroy', personal_program_chef, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Personal program chef', new_personal_program_chef_path %>
