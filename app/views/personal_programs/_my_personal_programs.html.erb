<tr>
	<th scope="row"><%= program.id %></th>
	<th><%= link_to program.program_name, program %></th>
	<th><%= program.note %></th>
	<th>
		<div style="display:inline-block;">
			<%= button_to 'Edit', edit_personal_program_path(program), :class => "btn btn-primary", :method => :get  %>
		</div>
		<div style="display:inline-block;">
			<%= button_to 'Delete', personal_program_user_personal_program_path(program ,@ku_user.user_personal_programs.find_by(personal_program_id: program).id), :class => "btn btn-default", :method => :delete %>
		</div>
	</th>
</tr>
