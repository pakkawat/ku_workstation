<p id="notice"><%= notice %></p>

<h1>Listing Personal Programs</h1>

<table>
  <thead>
    <tr>
      <th>Program name</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personal_programs.each do |personal_program| %>
      <tr>
        <td><%= personal_program.program_name %></td>
        <td><%= personal_program.note %></td>
        <td><%= link_to 'Show', personal_program %></td>
        <td><%= link_to 'Edit', edit_personal_program_path(personal_program) %></td>
        <td><%= link_to 'Destroy', personal_program, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Personal program', new_personal_program_path %>





<div class="row">
  <div class="col-sm-12">
    <span class="pull-left"><h1>Personal Programs</h1></span>
    <span class="pull-right">

    </span>
  </div>
</div>

<div class="panel panel-primary">
  <!-- Default panel contents -->
  <div class="panel-heading">Personal programs</div>
  <br>
  	<table id="" class="datatable_display table table-striped">
  		<thead>
  			<tr>
          <th>Program ID</th>
  				<th>Program name</th>
  				<th>Note</th>
  				<th>Action</th>
  			</tr>
  		</thead>
  		<tbody>
  			<%= render partial: "my_personal_programs", collection: @my_personal_programs, as: :program %>
  		</tbody>
	</table>
</div>


<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading">All personal programs</div>
  <br>
  	<table id="" class="datatable_display table table-striped">
  		<thead>
  			<tr>
          <th>Program ID</th>
  				<th>Program name</th>
  				<th>Note</th>
  				<th>Action</th>
  			</tr>
  		</thead>
  		<tbody>
  			<%= render partial: "all_personal_programs", collection: @all_personal_programs, as: :program %>
  		</tbody>
	</table>
</div>
