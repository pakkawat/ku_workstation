<tr>
	<th scope="row"><%= program.id %></th>
	<th><%= link_to program.program_name, program %></th>
	<th><%= program.note %></th>
	<th>
		<div style="display:inline-block;">
			<%= button_to 'Edit', edit_program_path(program), :class => "btn btn-primary", :method => :get  %>
		</div>
		<div style="display:inline-block;">
			<%= button_to 'Apply change', program_path(program)+"/apply_change", :class => "btn btn-warning", :method => :get, data: {title: "Apply change", confirm: 'It will take a long time to process, Are you sure?'} %>
		</div>
		<div style="display:inline-block;">
			<%= button_to 'Delete', program_path(program), data: {title: "Delete Program:#{program.program_name}", confirm: "Are you sure?" }, :class => "btn btn-default", :method => :delete %>
		</div>
	</th>
</tr>
