<div class="row">
  <div class="col-sm-12">
    <span class="pull-left"><h1><%= @subject.subject_name %> Programs</h1></span>
    <span class="pull-right">
      <h1>
        <%= button_to 'Apply change', program_apply_path, :class => "btn btn-primary", params: {'subject_id' => @subject.id.to_s} %>
      </h1>
    </span>
  </div>
</div>


<div class="panel panel-primary">
  <!-- Default panel contents -->
  <div class="panel-heading">Proram in this subject</div>
  <br>
  	<table id="" class="datatable_display table table-striped">
  		<thead>
  			<tr>
  				<th>Program ID</th>
  				<th>Program name</th>
  				<th>Note</th>
  				<th>Action</th>
  			</tr>
  		</thead>
  		<tbody>
  			<%= render partial: "subjectprogram", collection: @subjectprograms, as: :program %>
  		</tbody>
	</table>
</div>


<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading">Program List</div>
  <br>
  	<table id="" class="datatable_display table table-striped">
  		<thead>
  			<tr>
  				<th>Program ID</th>
  				<th>Program name</th>
  				<th>Note</th>
  				<th>Action</th>
  			</tr>
  		</thead>
  		<tbody>
  			<%= render partial: "programs", collection: @programs, as: :program %>
  		</tbody>
	</table>
</div>