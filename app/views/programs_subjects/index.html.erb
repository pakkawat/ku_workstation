<div class="row">
  <div class="col-sm-12">
    <span class="pull-left"><h1><%= @subject.subject_name %> Programs</h1></span>
    <span class="pull-right">
      <h1>
        <!--<div style="display:inline-block;">
          <% #link_to "Apply change", subject_path(@subject)+"/program_apply", :class => "btn btn-primary" %>
        </div>-->
        <div style="display:inline-block;">
          <%= button_to "Back to #{@subject.subject_name}", subject_path(@subject), :class => "btn btn-default", :method => :get %>
        </div>
      </h1>
    </span>
  </div>
</div>

<div class="panel panel-primary">
  <!-- Default panel contents -->
  <div class="panel-heading">Proram in this subject</div>
  <br>
  	<table id="" class="datatable_display table table-striped">
  		<thead>
  			<tr>
  				<th>Program name</th>
  				<th>Action</th>
          <th>State</th>
  			</tr>
  		</thead>
  		<tbody>
  			<%= render partial: "subjectprogram", collection: @subjectprograms, as: :program %>
  		</tbody>
	</table>
</div>


<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading">Program List</div>
  <br>
  	<table id="" class="datatable_display table table-striped">
  		<thead>
  			<tr>
  				<th>Program name</th>
  				<th>Action</th>
  			</tr>
  		</thead>
  		<tbody>
  			<%= render partial: "programs", collection: @programs, as: :program %>
  		</tbody>
	</table>
</div>
