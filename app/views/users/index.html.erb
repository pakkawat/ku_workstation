User index
<hr>
<% @users.each do |user| %>
<h2><%= link_to user.ku_id, user %></h2>
<p><%= user.firstname %></p>
<p><%= link_to "Edit", edit_post_path(user) %> | 
<%= link_to "Delete", user, :confirm => "Are you sure?", :method => :delete %>
</p>
<hr>
<% end %>

<p><%= link_to "Add new user", new_user_path %></p>
